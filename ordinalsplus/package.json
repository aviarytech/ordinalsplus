{
  "name": "ordinalsplus",
  "version": "0.1.0",
  "description": "A JavaScript/TypeScript library for working with BTCO DIDs and DID Linked Resources on the Bitcoin blockchain",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "test": "bun test",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "example": "bun examples/basic-usage.ts"
  },
  "keywords": [
    "bitcoin",
    "ordinals",
    "did",
    "btco",
    "decentralized",
    "identity",
    "linked-resources",
    "blockchain"
  ],
  "author": "Brian <example@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourname/ordinalsplus.git"
  },
  "bugs": {
    "url": "https://github.com/yourname/ordinalsplus/issues"
  },
  "homepage": "https://github.com/yourname/ordinalsplus#readme",
  "dependencies": {
    "jsonld": "^8.3.0",
    "multiformats": "^12.1.0"
  },
  "devDependencies": {
    "@types/jsonld": "^1.5.15",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "bun-types": "latest",
    "eslint": "^8.57.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}